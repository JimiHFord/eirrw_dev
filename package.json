{
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@selectize/selectize": "^0.15.2",
    "@tailwindcss/forms": "^0.5.3",
    "@types/datatables.net": "^1.10.24",
    "@types/datatables.net-fixedheader": "^3.1.5",
    "@types/datatables.net-rowgroup": "^1.1.2",
    "@types/jquery": "^3.5.16",
    "@types/selectize": "^0.12.35",
    "autoprefixer": "^10.4.13",
    "better-sqlite3": "^7.6.2",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "wrangler": "^3.21.0"
  },
  "scripts": {
    "install-theme": "cd ./themes/congo && npm ci",
    "prebuild": "npm run install-theme",
    "build": "hugo",
    "bootstrap-dev": "wrangler d1 execute D1_EIRRW --local --file bootstrap.sql",
    "dev-server": "wrangler pages dev --d1 D1_EIRRW --local -- hugo server -D --logLevel info --bind [::1]",
    "dev-server-prod": "wrangler pages dev --d1 D1_EIRRW --local -- hugo server -D --logLevel info --bind [::1] --environment production",
    "start": "npm run dev-server"
  },
  "dependencies": {
    "@cloudflare/pages-plugin-cloudflare-access": "^1.0.2",
    "@types/fuzzy-search": "^2.1.2",
    "fuzzy-search": "^3.2.1",
    "jquery": "^3.6.4",
    "tom-select": "^2.2.2"
  }
}
